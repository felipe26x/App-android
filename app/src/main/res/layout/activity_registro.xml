<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Registro">



    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="412dp"
        android:layout_height="300dp"
        android:layout_marginBottom="456dp"
        android:scaleType="centerCrop"
        android:src="@drawable/paisaje"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent">

    </ImageView>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="673dp"
        android:background="@drawable/disenoconstalogin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent">


        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="17dp"
            android:text="REGISTRATE"
            android:textColor="#2C2C2C"
            android:textSize="24dp"
            app:layout_constraintBottom_toTopOf="@+id/textingresa"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.146"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.307" />

        <EditText
            android:id="@+id/etnombres"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="39dp"
            android:layout_marginEnd="40dp"
            android:background="@drawable/disenoinputlogin"
            android:drawableStart="@drawable/ic_user"
            android:drawablePadding="20dp"
            android:hint="Nombres"
            android:inputType="textNoSuggestions"
            android:paddingLeft="20dp"
            android:textColor="#1B1B1B"
            android:textColorHighlight="#4CAF50"
            android:textColorHint="#7A7A7A"
            android:textColorLink="#18F121"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textingresa">

        </EditText>


        <EditText
            android:id="@+id/etapellidos"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="40dp"
            android:background="@drawable/disenoinputlogin"
            android:drawableStart="@drawable/ic_id_badge"
            android:drawablePadding="20dp"
            android:hint="Apellidos"
            android:inputType="textPersonName"
            android:paddingLeft="17dp"
            android:textColor="#1B1B1B"
            android:textColorHighlight="#4CAF50"
            android:textColorHint="#7A7A7A"
            android:textColorLink="#18F121"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etnombres" />

        <EditText
            android:id="@+id/etemail"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="34dp"
            android:layout_marginEnd="40dp"
            android:background="@drawable/disenoinputlogin"
            android:drawableStart="@drawable/ic_envelope"
            android:drawablePadding="20dp"
            android:hint="email"
            android:inputType="textWebEmailAddress|textEmailAddress"
            android:paddingLeft="17dp"
            android:textColor="#1B1B1B"
            android:textColorHighlight="#4CAF50"
            android:textColorHint="#7A7A7A"
            android:textColorLink="#18F121"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etapellidos" />

        <EditText
            android:id="@+id/etnumero_telefono"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="40dp"
            android:background="@drawable/disenoinputlogin"
            android:drawableStart="@drawable/ic_phone_call"
            android:drawablePadding="20dp"
            android:hint="numero telefono"
            android:inputType="number"
            android:paddingLeft="20dp"
            android:textColor="#1B1B1B"
            android:textColorHighlight="#4CAF50"
            android:textColorHint="#7A7A7A"
            android:textColorLink="#18F121"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etemail" />

        <EditText
            android:id="@+id/etcontraseña"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="27dp"
            android:layout_marginEnd="40dp"
            android:background="@drawable/disenoinputlogin"
            android:drawableStart="@drawable/ic_lock"
            android:drawablePadding="20dp"
            android:hint="Contraseña"
            android:inputType="textPassword"
            android:paddingStart="20dp"
            android:textColor="#1B1B1B"
            android:textColorHighlight="#4CAF50"
            android:textColorHint="#7A7A7A"
            android:textColorLink="#18F121"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etnumero_telefono" />

        <Button
            android:id="@+id/btnRegistrar"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="23dp"
            android:layout_marginEnd="40dp"
            android:background="@drawable/disenobotonlogin"
            android:gravity="center"
            android:text="Registrarme"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etcontraseña" />

        <TextView
            android:id="@+id/textingresa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="56dp"
            android:text="llena los campos para continuar"
            android:textColor="#393939"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.256"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>